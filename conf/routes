# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Map static resources from the /public folder to the /assets URL path
GET    /assets/*file           controllers.Assets.versioned(path="/public", file: Asset)
GET    /dist/*file             controllers.Assets.versioned(path="/public/dist", file: Asset)
GET    /lib/*file              controllers.Assets.versioned(path="/public/lib", file: Asset)
GET    /webjars/*file          controllers.WebJarAssets.at(file)


# api
GET   /data/:key             controllers.MyController.data(key: String)
POST  /postTodo              controllers.MyController.postTodo


# Other all route is index
GET    /                           controllers.MyController.index(path="")
GET    /*path                      controllers.MyController.index(path)