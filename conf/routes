# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Map static resources from the /public folder to the /assets URL path
GET    /assets/*file           controllers.Assets.versioned(path="/public", file: Asset)
GET    /dist/*file             controllers.Assets.versioned(path="/public/dist", file: Asset)
GET    /javascripts/*file      controllers.Assets.versioned(path="/public/javascripts", file: Asset)
GET    /images/*file           controllers.Assets.versioned(path="/public/images", file: Asset)
GET    /stylesheets/*file      controllers.Assets.versioned(path="/public/stylesheets", file: Asset)
GET    /lib/*file              controllers.Assets.versioned(path="/public/lib", file: Asset)
GET    /webjars/*file          controllers.WebJarAssets.at(file)


# api
GET   /data/:key               controllers.MyController.data(key: String)
POST  /postTodo                controllers.MyController.postTodo


POST   /login                  controllers.MyController.login
# Other all route
GET    /                       controllers.MyController.index(path="")
GET    /*path                  controllers.MyController.index(path)